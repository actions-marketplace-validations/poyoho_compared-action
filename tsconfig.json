{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "lib": ["esnext"],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "declaration": false
  }
}
